{
  "name": "demo-playwright-vue-server",
  "private": true,
  "version": "1.0.0",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "esrun src/server.ts",
    "prepare": "yarn build",
    "build": "tsc",
    "purge": "rimraf .node-persist",
    "serve": "node dist/server.js",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "express": "^4.19.2",
    "node-persist": "^4.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.11",
    "@types/node-persist": "^3.1.8",
    "eslint": "^8.57.0",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-vue": "^9.23.0",
    "esrun": "^3.2.26",
    "prettier": "^3.2.5",
    "typescript": "^5.5.3"
  }
}